<p>
  <b>Name:</b>
  <%=h @submission.name %>
</p>

<p>
  <b>Email:</b>
  <%=h @submission.email %>
</p>

<% @fields.each do |field| %>
<p>
  <b><%= field[:label] %></b>
  <%=h @submission[field[:name]] %>
</p>
<% end %>


<%= link_to 'Edit', edit_submission_path(@submission) %> |
<%= link_to 'Back', submissions_path %>